{
    "_version": "1.76.0",
    "sap.app": {
        "id": "uppersap.com.prototipo",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.20.0",
            "toolsId": "c27cc0f5-f05b-4bb2-9c6c-6db5bdfdf652"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.143.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "uppersap.com.prototipo.i18n.i18n"
                }
            },
            "datos": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/data.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "uppersap.com.prototipo.view",
                "async": true,
                "viewPath": "uppersap.com.prototipo.view"
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMain"
                    ]
                },
                {
                    "name": "RouteDetail",
                    "pattern": "Order/{orderPath}",  
                    "target": ["TargetDetail"]
                },
                {
                    "name": "RouteCreate",
                    "pattern": "create",
                    "target": ["TargetCreate"]
                }
            ],
            "targets": {
                "TargetMain": {
                    "id": "Main",
                    "name": "Main"
                },
                "TargetDetail": {
                    "viewType": "XML",
                    "viewName": "Detail",
                    "controlAggregation": "pages" 
                },
                "TargetCreate": {
                    "viewType": "XML",
                    "viewName": "Create",
                    "controlAggregation": "pages"
                }
            }
        },
        "rootView": {
            "viewName": "uppersap.com.prototipo.view.App",
            "type": "XML",
            "id": "App",
            "async": true
        }
    }
}